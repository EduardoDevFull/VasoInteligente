<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#cbedf5"
    tools:context=".TelaCadastro" >

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="99dp"
        android:layout_height="76dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="96dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/seta" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="411dp"
        android:layout_height="60dp"
        android:background="@drawable/barra_navegacao"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="411dp"
        android:layout_height="60dp"
        android:background="@drawable/barra_navegacao"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="259dp"
        android:layout_height="225dp"
        android:layout_marginBottom="-4dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/logobranca" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="407dp"
        android:layout_height="563dp"
        android:layout_marginStart="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout2"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="52dp"
            android:layout_marginBottom="6dp"
            android:fontFamily="@font/nokora_regular"
            android:text="Nome:"
            android:textColor="#000000"
            android:textSize="19sp"
            app:layout_constraintBottom_toTopOf="@+id/container_buttons"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginBottom="6dp"
            android:fontFamily="@font/nokora_regular"
            android:text="E-mail:"
            android:textColor="#000000"
            android:textSize="19sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="321dp"
            android:layout_height="50dp"
            android:layout_marginStart="44dp"
            android:layout_marginBottom="3dp"
            app:layout_constraintBottom_toTopOf="@+id/textView4"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/retangulo_dados" />

        <EditText
            android:id="@+id/editTextTextPersonName2"
            android:layout_width="311dp"
            android:layout_height="49dp"
            android:layout_marginStart="48dp"
            android:layout_marginBottom="3dp"
            android:ems="10"
            android:hint="Digite seu e-mail"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toTopOf="@+id/textView4"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/container_buttons"
            android:layout_width="321dp"
            android:layout_height="50dp"
            android:layout_marginStart="48dp"
            android:layout_marginBottom="460dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/retangulo_dados" />

        <EditText
            android:id="@+id/editTextTextPersonName"
            android:layout_width="311dp"
            android:layout_height="49dp"
            android:layout_marginStart="52dp"
            android:layout_marginBottom="12dp"
            android:ems="10"
            android:hint="Digite seu nome"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toTopOf="@+id/textView6"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="321dp"
            android:layout_height="50dp"
            android:layout_marginStart="43dp"
            app:layout_constraintBottom_toTopOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/retangulo_dados" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginBottom="6dp"
            android:fontFamily="@font/nokora_regular"
            android:text="Senha:"
            android:textColor="#000000"
            android:textSize="19sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView6"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginBottom="6dp"
            android:fontFamily="@font/nokora_regular"
            android:text="Confirme a senha:"
            android:textColor="#000000"
            android:textSize="19sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView3"
            app:layout_constraintStart_toStartOf="parent" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginStart="112dp"
            android:layout_marginBottom="145dp"
            android:background="@drawable/background_button"
            android:backgroundTint="#3CB371"
            android:fontFamily="@font/nokora_regular"
            android:text="Entrar"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/editTextTextPassword2"
            android:layout_width="311dp"
            android:layout_height="49dp"
            android:layout_marginStart="48dp"
            android:layout_marginBottom="1dp"
            android:ems="10"
            android:inputType="textPassword"
            app:layout_constraintBottom_toTopOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="321dp"
            android:layout_height="50dp"
            android:layout_marginStart="45dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/button"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/retangulo_dados" />

        <EditText
            android:id="@+id/editTextTextPassword3"
            android:layout_width="311dp"
            android:layout_height="49dp"
            android:layout_marginStart="52dp"
            android:layout_marginBottom="16dp"
            android:ems="10"
            android:inputType="textPassword"
            app:layout_constraintBottom_toTopOf="@+id/button"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/nokora_regular"
        android:text="Cadastre-se"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>